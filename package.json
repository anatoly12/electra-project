{
  "name": "electra",
  "version": "1.0.0",
  "description": "Isomorphic React scaffold for Electron, NodeJS and Web",
  "keywords": [
    "fs",
    "electron",
    "react",
    "firebase"
  ],
  "homepage": "https://github.com/olegkleiman/electra",
  "author": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/olegkleiman/electra.git"
  },
  "bugs": {
    "url": "https://github.com/olegkleiman/electra/issues"
  },
  "license": "ISC",
  "main": "hook.js",
  "jest": {
    "verbose": true,
    "collectCoverage": true
  },
  "scripts": {
    "test": "jest --setupTestFrameworkScriptFile=raf/polyfill",
    "ci-test": "istanbul cover _mocha && cat ./coverage/lcov.info | coveralls",
    "server:start:web": "nodemon hookWeb.js",
    "server:debug:web": "node --inspect-brk hookWeb.js",
    "server:start": "electron -r babel-register hook.js",
    "debug": "electron --inspect-brk -r babel-register .",
    "client:build:web": "webpack --config webpack.config.web.js",
    "client:build": "webpack -d --progress",
    "client:start": "webpack-dev-server -d --progress --colors --watch --port 8080",
    "client:start:web": "webpack-dev-server --config webpack.config.web.js -d --progress --colors --watch --port 8080",
    "build": "electron-packager . --out=dist --asar --overwrite --all",
    "build-main": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.main.prod.js --colors"
  },
  "dependencies": {
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.7",
    "devtron": "^1.4.0",
    "electron": "^1.7.9",
    "electron-debug": "^1.4.0",
    "electron-devtools-installer": "^2.2.1",
    "electron-packager": "^9.1.0",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "firebase": "^4.6.2",
    "firebase-admin": "^5.5.0",
    "grpc": "^1.7.1",
    "material-ui": "^0.19.4",
    "material-ui-icons": "^1.0.0-beta.17",
    "moment": "^2.19.2",
    "nodemon": "^1.12.1",
    "raf": "^3.4.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "regenerator-runtime": "^0.11.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-flow": "^6.23.0",
    "cross-env": "^5.1.1",
    "jest": "^21.2.1",
    "react-test-renderer": "^16.1.1",
    "xo": "^0.18.2"
  }
}
